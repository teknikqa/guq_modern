name = Georgetown University SFS Qatar Theme
description = The new theme for Georgetown University SFS Qatar Theme. Based on Bootstrap 3
core = 7.x
base theme = bootstrap

; Regions
regions[top_nav]        = 'Top Navigation'
regions[top_header]        = 'Top Header'
regions[navigation]     = 'Navigation'
regions[header]         = 'Top Bar'
regions[header_mobile]  = 'Top Bar - Mobile'
regions[highlighted]    = 'Highlighted'
regions[help]           = 'Help'
regions[content]        = 'Content'
regions[top_content]    = 'Top Content'
regions[bottom_content] = 'Bottom Content'
regions[sidebar_first]  = 'First Side Bar'
regions[sidebar_second] = 'Second Side Bar'
regions[sidebar_default] = 'First Side Bar Default content'
regions[right_sidebar_default] = 'Right Default content'
regions[center_top_default] = 'Center Top Default'
regions[top_footer]  = 'Top Footer'
regions[footer_area] = 'Footer'
regions[footer_menu] = 'Footer Menu'

